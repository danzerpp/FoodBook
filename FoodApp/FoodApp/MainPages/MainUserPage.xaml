<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FoodApp.MainPages.MainUserPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>


    </ContentPage.Content>
    <ContentPage.ToolbarItems>
        <ToolbarItem Text="Wyloguj się" Priority="0" Order="Secondary" Clicked="ToolbarItem_Clicked" />
        <ToolbarItem Text="add" Priority="0" Order="Primary" Clicked="ToolbarItem_Clicked" IconImageSource="add.png" />
        <ToolbarItem Text="edit" Priority="1" Order="Primary" Clicked="ToolbarItem_Clicked" IconImageSource="edit.png"/>
        <ToolbarItem Text="dinner" Priority="2" Order="Primary" Clicked="ToolbarItem_Clicked" IconImageSource="dinner.png"/>
        <ToolbarItem Text="recipes" Priority="3" Order="Primary" Clicked="ToolbarItem_Clicked" IconImageSource="recipes.png"/>
    </ContentPage.ToolbarItems>

 

</ContentPage>